<!DOCTYPE html>
<html>
  <head>
    <title>My Developer Skills</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>My Developer Skills</h1>
    <ul>
      <% skills.forEach(function(s, idx) { %>
        <li>
          <a href="/skills/<%= idx %>"><%= s.skill %></a>
          -
          <%= s.have ? 'I can do it!' : 'I'm working on it' %>
        </li>
      <% }); %>
    </ul>
  </body>
</html>
